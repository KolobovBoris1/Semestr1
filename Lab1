#include <iostream>
#include <cmath>

using namespace std;

int main() {
    setlocale(LC_ALL, "Russian");
    double a = 100, b=0.001, m,n,k; //где m = pow((a-b),4), n = pow(a,4) - 4 * pow (a,3) * b + 6 * pow(a,2) * pow(b,2), k = pow(b,4) - 4 * a * pow(b,3)
    m = pow((a-b),4);
    n = pow(a,4) - 4 * pow (a,3) * b + 6 * pow(a,2) * pow(b,2);
    k = pow(b,4) - 4 * a * pow(b,3);
    cout.precision(15);
    cout <<"m = " <<fixed<<m<<endl;
    cout <<"n = " <<fixed<<n<<endl;
    cout <<"k = " <<fixed<<k<<endl;
    cout <<"answer ((m-n)/k): " <<fixed<<((m-n)/k);
    return 0;
}
